<div class="container">
  <h1>Your Bookings:</h1>
  <div class="row">

      <div class="col-sm-6">
        <% @bookings.each do |booking| %>
        <ul>
          <li>
            <%= booking.yurt.title %>
          </li>
          <li>
            <%= booking.yurt.description %>
          </li>
          <li>
            <%= booking.booked_from %>
            <%= booking.booked_to %>
          </li>
        </ul>
    <% end %>
      </div>
  </div>
</div>


